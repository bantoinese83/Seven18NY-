{
  "rewrites": [
    { "source": "/images/(.*)", "destination": "/images/$1" },
    { "source": "/gallery/(.*)", "destination": "/gallery/$1" },
    { "source": "/videos/(.*)", "destination": "/videos/$1" },
    { "source": "/favicon(.*)", "destination": "/favicon$1" },
    { "source": "/vite.svg", "destination": "/vite.svg" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
